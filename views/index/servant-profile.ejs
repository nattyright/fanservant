<div id="col-left" style="flex: 5.5%">
    <div id="button-close-profile" style="flex: 25%"><span>Close</span></div>
    <div id="button-edit-page" style="flex: 25%">
      <i class="fa-solid fa-pen-to-square"></i><span>Edit</span>
    </div>
    <div id="button-create-page" style="flex: 25%">
      <i class="fa-solid fa-wand-magic-sparkles"></i><span>Create</span>
    </div>
    <div style="flex: 25%"><span>Favorite</span></div>
</div>


<div id="col-mid" style="flex: 32.5%">
    <img id="pf-charagraph-base" src="">
    <img id="pf-cardURL" src="">
    <img id="pf-charagraph-frame" src="">
    <img id="pf-charagraph-class" src="">
    <div id="pf-info-servantClass"></div>
    <div id="pf-info-servantClass-underline"></div>
    <div id="pf-info-servantName"></div>
</div>


<div id="col-right" style="flex: 62%">
    <div id="profile-name">
        <div id="pf-servantName"></div>
        <div id="pf-servantClass"></div>
        <div id="pf-servantID" style="display:none"></div>
    </div>
    <div id="profile-nav">
        <div style="flex: 25%" class="profile-nav active" data-slide="0">Status</div>
        <div style="flex: 25%" class="profile-nav" data-slide="1">Profile</div>
        <div style="flex: 25%" class="profile-nav" data-slide="2">Gallery</div>
        <div style="flex: 25%" class="profile-nav" data-slide="3">Voice</div>
    </div>
    <div id="profile-content">

        <div class="profile-content active" data-slide="0">
